Host *
     ForwardAgent yes	
     AddKeysToAgent yes
     IdentitiesOnly yes
     # UseKeychain yes  # macOS only - uncomment if on macOS
     IdentityFile ~/.ssh/id_rsa

# Load public host snippets first…
Include ~/.ssh/config.d/public/*.conf
# …then load private snippets that can override
Include ~/.ssh/config.d/private/*.conf

